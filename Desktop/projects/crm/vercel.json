{
  "version": 2,
  "buildCommand": "./build.sh",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install --global react-scripts",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/logo192.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 